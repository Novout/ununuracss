{
  "name": "ununura-monorepo",
  "author": "Novout",
  "license": "MIT",
  "description": "A Simplified Atomic CSS Engine",
  "private": true,
  "scripts": {
    "build": "lerna run build:minify",
    "dev": "lerna run dev",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "demo:vue": "pnpm build && pnpm -r --filter=./examples/* run demo:vue",
    "demo:nuxt": "pnpm build && pnpm -r --filter=./examples/* run demo:nuxt",
    "demo:svelte": "pnpm build && pnpm -r --filter=./examples/* run demo:svelte",
    "demo:react": "pnpm build && pnpm -r --filter=./examples/* run demo:react",
    "demo:preact": "pnpm build && pnpm -r --filter=./examples/* run demo:preact",
    "preview:vue": "pnpm build && pnpm -r --filter=./examples/* run preview:vue",
    "preview:nuxt": "pnpm build && pnpm -r --filter=./examples/* run preview:nuxt",
    "preview:svelte": "pnpm build && pnpm -r --filter=./examples/* run preview:svelte",
    "preview:react": "pnpm build && pnpm -r --filter=./examples/* run preview:react",
    "preview:preact": "pnpm build && pnpm -r --filter=./examples/* run preview:preact",
    "patch": "generi log patch",
    "minor": "generi log minor",
    "major": "generi log major",
    "prepare": "husky install",
    "prepublish": "lerna run build:minify"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "@tsconfig/svelte": "^3.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.26.2",
    "babel-jsx-to-ast-fragmented": "^0.1.0",
    "concurrently": "^7.6.0",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.12",
    "fontaine": "^0.2.3",
    "generi": "^0.5.2",
    "hast": "^1.0.0",
    "hast-util-from-html": "^1.0.0",
    "husky": "^8.0.2",
    "lerna": "^6.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "svelte": "^3.54.0",
    "svelte-check": "^2.10.0",
    "tslib": "^2.4.1",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-inspect": "^0.7.11",
    "vitest": "^0.27.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "preact": "^10.11.3",
    "@preact/preset-vite": "^2.4.0"
  }
}
