{
  "name": "ununura-monorepo",
  "author": "Novout",
  "license": "MIT",
  "description": "A Simplified Atomic CSS Engine",
  "private": true,
  "scripts": {
    "build": "lerna run build",
    "dev": "lerna run dev",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "demo:vue": "pnpm build && pnpm -r --filter=./examples/* run demo:vue",
    "demo:nuxt": "pnpm build && pnpm -r --filter=./examples/* run demo:nuxt",
    "preview:vue": "pnpm build && pnpm -r --filter=./examples/* run preview:vue",
    "preview:nuxt": "pnpm build && pnpm -r --filter=./examples/* run preview:nuxt",
    "patch": "generi log patch",
    "minor": "generi log minor",
    "major": "generi log major",
    "prepare": "husky install",
    "prepublish": "pnpm run build"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.26.2",
    "concurrently": "^7.6.0",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.12",
    "fontaine": "^0.2.3",
    "generi": "^0.5.2",
    "hast": "^1.0.0",
    "hast-util-from-html": "^1.0.0",
    "husky": "^8.0.2",
    "lerna": "^6.3.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite-plugin-inspect": "^0.7.11",
    "vitest": "^0.27.0"
  }
}
