{
  "name": "ununura-monorepo",
  "author": "Novout",
  "license": "MIT",
  "version": "0.0.0",
  "description": "Simplified Atomic CSS Engine",
  "private": true,
  "scripts": {
    "build": "pnpm -r --filter=./packages/* run build && node ./packages/core/dist/index.js",
    "dev": "pnpm -r --filter=./packages/* --parallel run dev",
    "release": "bumpp package.json packages/**/package.json",
    "test": "vitest run --coverage",
    "test:dev": "vitest",
    "demo:vue": "pnpm build && pnpm -r --filter=./examples/* run demo:vue",
    "preview:vue": "pnpm build && pnpm -r --filter=./examples/* run preview:vue",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.26.2",
    "bumpp": "^8.2.1",
    "concurrently": "^7.6.0",
    "execa": "^6.1.0",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vite-plugin-inspect": "^0.7.11",
    "vitest": "^0.26.2"
  },
  "dependencies": {
    "hast": "^1.0.0",
    "hast-util-from-html": "^1.0.0"
  }
}
