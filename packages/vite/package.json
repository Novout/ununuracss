{
  "name": "ununura",
  "version": "0.2.0",
  "description": "Atomic CSS Engine.",
  "author": "Novout",
  "license": "MIT",
  "homepage": "https://github.com/Novout/ununuracss",
  "keywords": [
    "ununura",
    "atomic-css",
    "css"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.cjs"
    }
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:minify": "tsup --minify"
  },
  "peerDependencies": {
    "vite": "^3.0.0-0 || ^4.0.0"
  },
  "dependencies": {
    "fontaine": "^0.2.3",
    "ununura-engine": "workspace:*",
    "ununura-shared": "workspace:*"
  },
  "devDependencies": {
    "vite": "^4.0.3"
  }
}
