{
  "name": "astro-ununura",
  "version": "1.6.5",
  "description": "A Atomic CSS Engine Integration for Astro.",
  "author": "Novout",
  "license": "MIT",
  "homepage": "https://ununura.com",
  "repository": "https://github.com/Novout/ununuracss",
  "keywords": [
    "astro-integration"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:minify": "tsup --minify"
  },
  "dependencies": {
    "ununura-config": "workspace:*",
    "ununura-engine": "workspace:*",
    "ununura-shared": "workspace:*",
    "vite-plugin-ununura": "workspace:*"
  }
}
