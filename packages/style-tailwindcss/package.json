{
  "name": "ununura-style-tailwindcss",
  "version": "1.6.17",
  "description": "TailwindCSS Style for UnunuraCSS.",
  "author": "Novout",
  "license": "MIT",
  "homepage": "https://ununura.com",
  "keywords": [
    "ununura",
    "atomic-css",
    "tailwindcss",
    "css"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:minify": "tsup --minify"
  },
  "dependencies": {
    "ununura-shared": "workspace:*"
  }
}
