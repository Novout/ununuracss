{
  "name": "ununura-engine",
  "version": "1.6.13",
  "description": "Atomic CSS Engine.",
  "author": "Novout",
  "license": "MIT",
  "homepage": "https://ununura.com",
  "keywords": [
    "ununura",
    "ununura-css",
    "atomic-css",
    "css",
    "vue",
    "vite",
    "nuxt"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:minify": "tsup --minify"
  },
  "dependencies": {
    "autoprefixer": "^10.4.13",
    "nanoid": "3.0.0",
    "postcss": "^8.4.21",
    "ununura-font-stacks": "workspace:*",
    "ununura-shared": "workspace:*",
    "ununura-vue-sfc": "workspace:*"
  }
}
