{
  "name": "nuxt-ununura",
  "version": "1.6.10",
  "description": "Atomic CSS Engine for Nuxt3",
  "author": "Novout",
  "license": "MIT",
  "repository": "https://github.com/Novout/ununuracss",
  "homepage": "https://ununura.com",
  "keywords": [
    "ununuracss",
    "atomic-css",
    "nuxt-module",
    "nuxt",
    "nuxt3"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "build:minify": "tsup --minify"
  },
  "dependencies": {
    "@nuxt/kit": "^3.3.1",
    "ununura-shared": "workspace:*",
    "vite-plugin-ununura": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/schema": "^3.3.1"
  }
}
